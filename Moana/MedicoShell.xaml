<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Moana.MedicoShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:Moana.View"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate view:MedicoHomePage}"
        Route="MedicoHomePage" />

    <ShellContent
        Title="ListadoPacientes"
        ContentTemplate="{DataTemplate view:ListadoPacientes}"
        Route="ListadoPacientes" />

    <ShellContent
        Title="Prescripciones"
        ContentTemplate="{DataTemplate view:Prescripciones}"
        Route="Prescripciones" />
</Shell>
